<library name="standard" contains="type" target="c">
    <template name="comb">
	{{ placeholder("func") }}
    </template>
    <template name="moore">
{{ placeholder("ns_func") }}
{{ placeholder("od_func") }}
    </template>
    <template name="+">
	{{ret.ifName}} =  {{arg1.ifName}} + {{arg2.ifName}};
    </template>
    <template name="id">
	{{ret.ifName}} = {{arg.ifName}};
    </template>
    <template name="stdinRead">
	<requirement include="stdio.h"/>
scanf("{{form.macroVal}}", &amp;{{inp.ifName}});
    </template>
    <template name="stdoutWrite">
	<requirement include="stdio.h"/>
printf("{{form.macroVal}}", {{out.ifName}});
    </template>
</library>
